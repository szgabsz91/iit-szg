{
  "name": "iit-www",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "i18n:generate": "ng xi18n --output-path locale",
    "prestart": "./scripts/generate-static-content.js en",
    "start": "ng serve",
    "prestart:hu": "./scripts/generate-static-content.js hu",
    "start:hu": "ng serve --configuration=hu",
    "prebuild": "./scripts/generate-static-content.js en",
    "build": "ng build --outputPath=dist/iit-www/en",
    "postbuild": "cp deployment/github-pages/* dist/iit-www",
    "prebuild:hu": "./scripts/generate-static-content.js hu",
    "build:hu": "ng build --configuration=hu --outputPath=dist/iit-www/hu",
    "postbuild:hu": "cp deployment/github-pages/* dist/iit-www",
    "prebuild:prod": "./scripts/generate-static-content.js en",
    "build:prod": "ng build --configuration=production --outputPath=dist/iit-www/en",
    "postbuild:prod": "cp deployment/github-pages/* dist/iit-www",
    "prebuild:hu:prod": "./scripts/generate-static-content.js hu",
    "build:hu:prod": "ng build --configuration=production,hu --outputPath=dist/iit-www",
    "postbuild:hu:prod": "cp deployment/github-pages/* dist/iit-www",
    "test": "ng test --watch=false --code-coverage",
    "test:watch": "ng test",
    "lint": "ng lint",
    "pree2e": "./scripts/generate-static-content.js",
    "e2e": "ng e2e",
    "prepare-deployment:gh-pages": "rm -rf dist && yarn build:prod --base-href https://szgabsz91.github.io/iit-www/en/ && yarn build:hu:prod --base-href https://szgabsz91.github.io/iit-www/ --deleteOutputPath=false",
    "ci": "yarn lint && yarn test && yarn e2e && yarn prepare-deployment:gh-pages"
  },
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/localize": "^9.1.1",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "core-js": "^3.6.4",
    "handlebars": "^4.3.0",
    "kind-of": "^6.0.3",
    "minimist": "^1.2.3",
    "mixin-deep": "^1.3.2",
    "ngx-markdown": "^9.0.0",
    "rxjs": "~6.5.4",
    "set-value": "^2.0.1",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.12.31",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "zip-folder": "1.0.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.8.1",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3",
    "webdriver-manager": "~12.1.7"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=12.2.0",
    "yarn": ">=1.16.0"
  }
}
